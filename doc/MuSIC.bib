@article{Pradeep2020CharacterizationQualificationLPBF,
  title = {Characterization and {{Qualification}} of {{LPBF Additively Manufactured AISI-316L Stainless Steel Brackets}} for {{Aerospace Application}}},
  author = {Pradeep, P. I. and Kumar, V. Anil and Sriranganath, A. and Singh, Satish Kumar and Sahu, Ankit and Kumar, T. Sasi and Narayanan, P. Ramesh and Arumugam, M. and Mohan, M.},
