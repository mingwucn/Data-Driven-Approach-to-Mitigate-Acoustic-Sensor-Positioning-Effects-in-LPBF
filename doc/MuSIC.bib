@inproceedings{Pratt2023SituBottomTemperatureMeasurements,
  title = {In-{{Situ Bottom-Up Temperature Measurements}} for {{Laser Powder Bed Fusion Metal Additive Manufacturing}}},
  booktitle = {{{ASME}} 2023 {{Heat Transfer Summer Conference}}},
