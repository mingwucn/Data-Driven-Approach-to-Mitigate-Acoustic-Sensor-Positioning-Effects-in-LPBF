@article{Pandiyan2022DeepLearningbasedMonitoring,
  title = {Deep Learning-Based Monitoring of Laser Powder Bed Fusion Process on Variable Time-Scales Using Heterogeneous Sensing and Operando {{X-ray}} Radiography Guidance},
  author = {Pandiyan, Vigneashwara and Masinelli, Giulio and Claire, Navarre and {Le-Quang}, Tri and {Hamidi-Nasab}, Milad and {de Formanoir}, Charlotte and Esmaeilzadeh, Reza and Goel, Sneha and Marone, Federica and Log{\'e}, Roland and Van Petegem, Steven and Wasmer, Kilian},
  year = {2022},
