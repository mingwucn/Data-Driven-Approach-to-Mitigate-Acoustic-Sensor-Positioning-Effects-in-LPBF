@inproceedings{Imani2018LayerwiseProcessQuality,
  title = {Layerwise {{In-Process Quality Monitoring}} in {{Laser Powder Bed Fusion}}},
  booktitle = {{{ASME}} 2018 13th {{International Manufacturing Science}} and {{Engineering Conference}}},
  author = {Imani, Farhad and Gaikwad, Aniruddha and Montazeri, Mohammad and Rao, Prahalada and Yang, Hui and Reutzel, Edward},
