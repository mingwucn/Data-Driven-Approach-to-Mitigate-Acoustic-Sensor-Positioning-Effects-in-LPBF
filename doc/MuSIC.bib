  title = {In-{{Situ Crack}} and {{Keyhole Pore Detection}} in {{Laser Directed Energy Deposition Through Acoustic Signal}} and {{Deep Learning}}},
  author = {Chen, Lequn and Yao, Xiling and Tan, Chaolin and He, Weiyang and Su, Jinlong and Weng, Fei and Chew, Youxiang and Ng, Nicholas Poh Huat and Moon, Seung Ki},
  year = {2022},
